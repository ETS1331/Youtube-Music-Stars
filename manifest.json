{
  "name": "Hello Extensions",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "hello.html"
  },
  "permissions":[
	  "webNavigation",
	  "scripting",
	  "tabs"
  ],
  "host_permissions":[
	  "https://music.youtube.com/*"
  ],

  "content_scripts": [
	  {
		  "js": ["content.js"],
		  "matches": [
			  "https://music.youtube.com/*"
		  ]
	  }
  ],
  "background": {
	  "service_worker": "background.js"
  }
}
