{
  "name": "Hello Extensions",
  "externally_connectable": {
	  "matches": [
		  "https://music.youtube.com/*"
	  ]
  },
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "hello.html"
  },
  "permissions":[
	  "webNavigation",
	  "scripting",
	  "tabs",
	  "storage"
  ],
  "host_permissions":[
	  "https://music.youtube.com/*"
  ],

  "content_scripts": [
	  {
		  "js": ["content.js"],
		  "css": ["stars.css"],
		  "matches": [
			  "https://music.youtube.com/*"
		  ]
	  }
  ],
  "background": {
	  "service_worker": "background.js"
  },
  "web_accessible_resources": [{
	  "resources": ["stardatabase.js"],
	  "matches": [
		  "https://music.youtube.com/*"
	  ]
}]
}
